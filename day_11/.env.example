# YandexCloud API Credentials
# Get your API key and folder ID from https://cloud.yandex.com/en/docs/iam/operations/iam-token/create

YANDEXCLOUD_API_KEY=your_api_key_here
YANDEXCLOUD_FOLDER_ID=your_folder_id_here

# YandexCloud Model Settings
# Available models: yandexgpt-lite/latest, yandexgpt/latest, yandexgpt-pro/latest
YANDEXCLOUD_MODEL=yandexgpt-lite/latest

# Temperature (0.0 - 1.0, higher = more creative, lower = more focused)
YANDEXCLOUD_TEMPERATURE=0.7